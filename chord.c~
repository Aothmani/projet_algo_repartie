#include <stdio.h>
#include <stdlib.h>
#include <mpi.h>

#define NB_SITE 5


void simulateur(void)
{
	srand(getpid());

	int id[NB_SITE], i, val, j;
	memset(id, -1, NB_SITE);
	
	for(i = 0; i < NB_SITE; i++){
		val = rand()%64;
		for(j = 0; j < NB_SITE; j++){
			if(val == id[j]){
				val = rand%64;
				j = 0;
			}
		}
	}
}
